<jnlp  spec="1.0+" codebase="" href="gdal.jnlp">

    <!--
        Provides a description of the WorldWind GDAL library.
    -->
    <information>
        <title>NASA WorldWind Java GDAL</title>
        <vendor>NASA</vendor>
        <homepage href="https://worldwind.arc.nasa.gov"/>
        <offline-allowed/>
    </information>

    <!--
        Requests a trusted application environment. The WorldWind GDAL library requires access to the local filesystem.
        All WorldWind JAR files are signed with a digital signature associated with NASA.
    -->
    <security>
        <all-permissions/>
    </security>

    <!--
        Specifies the resources required by the WorldWind GDAL library:
        * WorldWind GDAL JAR file.
        * WorldWind GDAL data JAR file.
        * WorldWind GDAL native libraries.
    -->
    <resources>
        <jar href="gdal.jar"/>
        <jar href="gdaldata.jar"/>
    </resources>
    <resources os="Windows" arch="x86">
        <nativelib href="gdal-natives-windows-i586.jar"/>
    </resources>
    <resources os="Windows" arch="amd64">
        <nativelib href="gdal-natives-windows-amd64.jar"/>
    </resources>
    <resources os="Windows" arch="x86_64">
        <nativelib href="gdal-natives-windows-amd64.jar"/>
    </resources>
    <resources os="SunOS" arch="sparc">
        <nativelib href="gdal-natives-solaris-sparc.jar"/>
    </resources>
    <resources os="SunOS" arch="sparcv9">
        <nativelib href="gdal-natives-solaris-sparcv9.jar"/>
    </resources>
    <resources os="SunOS" arch="x86">
        <nativelib href="gdal-natives-solaris-i586.jar"/>
    </resources>
    <resources os="SunOS" arch="amd64">
        <nativelib href="gdal-natives-solaris-amd64.jar"/>
    </resources>
    <resources os="SunOS" arch="x86_64">
        <nativelib href="gdal-natives-solaris-amd64.jar"/>
    </resources>
    <resources os="Linux" arch="i386">
        <nativelib href="gdal-natives-linux-i586.jar"/>
    </resources>
    <resources os="Linux" arch="x86">
        <nativelib href="gdal-natives-linux-i586.jar"/>
    </resources>
    <resources os="Linux" arch="amd64">
        <nativelib href="gdal-natives-linux-amd64.jar"/>
    </resources>
    <resources os="Linux" arch="x86_64">
        <nativelib href="gdal-natives-linux-amd64.jar"/>
    </resources>
    <resources os="Mac OS X" arch="ppc">
        <nativelib href="gdal-natives-macosx-ppc.jar"/>
    </resources>
    <resources os="Mac OS X" arch="i386">
        <nativelib href="gdal-natives-macosx-universal.jar"/>
    </resources>
    <resources os="Mac OS X" arch="x86_64">
        <nativelib href="gdal-natives-macosx-universal.jar"/>
    </resources>

    <!--
        Indicates that this Java Web Start file describes a component. This element is intentionally left blank, since
        it is used only as an indicator.
    -->
    <component-desc/>

</jnlp>
